{
    "compilerOptions": {
		"jsx": "react-jsx",
		"lib": [
			"DOM",
			"ESNext",
			"ESNext.Array",
			"ESNext.AsyncIterable",
			"ES2023",
			"ES2022",
			"ES2021",
			"ES2020",
			"ES2019",
			"ES2016",
			"ES2015",
			"DOM.Iterable"
		],
		"skipDefaultLibCheck": true,
		"skipLibCheck": true
	},
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@starter-kit/tsconfig/nextjs.json",
	"include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
	"exclude": ["node_modules"],
}
